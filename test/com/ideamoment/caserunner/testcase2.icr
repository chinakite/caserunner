group sinamail {
    login, writeEmail
};

case openLoginPage {
    get "http://mail.sina.com.cn";
    wait 2000;
}

case login {    
    assert "#freename" exists;
    assert "#freepassword" exists;
    input "ideacaserunner@sina.com" to "#freename";
    input "IdeaCaseRunner1" to "#freepassword";
    click ".loginBtn";
    wait 5000;
};

case writeEmail {
    click ".wrWriteBtn a" when ".wrWriteBtn a" exists timeout 5;
}